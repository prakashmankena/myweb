currentBuild.displayName="HDFCIT1-#"+currentBuild.number
pipeline{
  agent any
  environment{
    PATH = "/opt/apache-maven-3.6.3/bin:$PATH"
    }
  stages{
    stage("Wel-Come"){
      steps{
        echo "Welcome to Jenkins Declarative Git"
        }
      }
      
    stage("GIT-CLone"){
      steps{
        git credentialsId: 'Git-Hub', url: 'https://github.com/prakashmankena/myweb/'
      }
    }
    
    stage("Build-WAR"){
      steps{
        sh "mvn clean"
      }
    }
  }
}
