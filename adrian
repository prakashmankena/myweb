currentBuild.displayName = "HDFCIT1-#"+currentBuild.number
environment{
PATH = "/opt/apache-maven-3.6.3/bin/mvn:$PATH"
}
pipeline{
    agent any
    stages{
      stage("WELCOME"){
      steps{
          echo "Welcome to Jenkins Declrative" 
      }
     }
     
     stage("Git-Clone"){
     steps{
            git credentialsId: 'git', url: 'https://github.com/prakashmankena/myweb/'
     
      }
     }
     
     stage("Build"){
     steps{
        sh "mvn clean"
     }
     }
    }

}
